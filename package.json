{
  "name": "agent-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Starter template for building AI chat agents on Cloudflare",
  "keywords": [
    "agents",
    "ai",
    "cloudflare"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "start": "vite dev",
    "deploy": "vite build && wrangler deploy",
    "types": "wrangler types env.d.ts --include-runtime false",
    "format": "oxfmt --write .",
    "lint": "oxlint src/",
    "check": "oxfmt --check . && oxlint src/ && tsc"
  },
  "dependencies": {
    "@cloudflare/ai-chat": "^0.1.2",
    "@cloudflare/kumo": "^1.5.1",
    "@phosphor-icons/react": "^2.1.10",
    "agents": "^0.5.0",
    "ai": "^6.0.89",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "streamdown": "^2.2.0",
    "workers-ai-provider": "^3.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.25.1",
    "@cloudflare/workers-types": "^4.20260217.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wrangler": "4.66.0"
  }
}
